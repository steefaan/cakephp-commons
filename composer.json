{
	"name" : "php-engineer/cakephp-commons",
	"description": "A CakePHP 3 plugin with a lot of reusable code.",
	"type": "cakephp-plugin",
	"license" : "MIT",
	"authors": [
		{
			"name": "Stefan Dickmann",
			"homepage": "http://php-engineer.de"
		}
	],
	"require" : {
		"php": ">=5.4",
		"composer/installers": "~1.0",
		"cakephp/cakephp": "3.0.*-dev"
	},
	"autoload": {
		"psr-4": {
			"Commons\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Commons\\Test\\": "tests"
		}
	},
	"extra": {
		"installer-name": "Commons"
	}
}
